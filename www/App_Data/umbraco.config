          <NewsItem id="1907" key="8c5e9774-857c-449c-81d1-b5133b634f09" parentID="1615" level="5" creatorID="5" sortOrder="17" createDate="2014-04-23T11:13:54" updateDate="2014-05-01T08:02:29" nodeName="The simple truth" urlName="the-simple-truth" path="-1,1082,1104,1277,1615,1907" isDoc="" nodeType="1530" creatorName="belindam" writerName="clarec" writerID="19" template="1529" nodeTypeAlias="NewsItem">
