    <ContentPage id="2427" key="db22ad5f-2ece-4306-ac82-25d532c0a588" parentID="1082" level="2" creatorID="23" sortOrder="20" createDate="2015-05-01T11:19:27" updateDate="2015-05-08T10:50:48" nodeName="Your investments" urlName="your-investments" path="-1,1082,2427" isDoc="" nodeType="1127" creatorName="rpowell" writerName="rpowell" writerID="23" template="1129" nodeTypeAlias="ContentPage">
