          <NewsItem id="1893" key="ecdbd1f3-3cf0-4974-83ff-2c2da8dcf248" parentID="1798" level="5" creatorID="20" sortOrder="6" createDate="2014-04-23T10:54:01" updateDate="2014-04-30T09:47:48" nodeName="Economic and Market Summary - September 2012" urlName="economic-and-market-summary-september-2012" path="-1,1082,1104,1277,1798,1893" isDoc="" nodeType="1530" creatorName="peleinag" writerName="clarec" writerID="19" template="1529" nodeTypeAlias="NewsItem">
